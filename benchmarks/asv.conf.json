{
    "version": 1,
    "project": "imas-mcp",
    "repo": "..",
    "branches": ["main", "HEAD"],
    "dvcs": "git",
    "environment_type": "virtualenv",
    "pythons": ["3.12"],
    "matrix": {},
    "install_command": ["python", "-m", "pip", "install", "{wheel_file}"],
    "uninstall_command": ["python", "-m", "pip", "uninstall", "-y", "{packages}"],
    "build_command": ["uv", "build", "--wheel", "-o", "{build_cache_dir}"],
    "benchmark_dir": "benchmarks",
    "env_dir": ".asv/env",
    "results_dir": ".asv/results",
    "html_dir": ".asv/html",
    "build_cache_size": 8
}
